<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="MenuTheme" TargetType="Border">
        <Setter Property="Background" Value="#146EC1"/>
        <Setter Property="BorderBrush" Value="#505F94"/>
        <Setter Property="BorderThickness" Value="2"/>
    </Style>
    <LinearGradientBrush x:Key="MainBackground" EndPoint="0,0" StartPoint="1, 1">
        <GradientStop Color="#FFA9FF00"/>
        <GradientStop Color="#31FAF520" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="Navbar" Color="#FFE8CA89"/>
    <SolidColorBrush x:Key="Searchbar" Color="#FF017A95"/>
    <SolidColorBrush x:Key="GenRes" Color="#FF09A3D7"/>
    <SolidColorBrush x:Key="ChangeCov" Color="#4C05280B"/>
</ResourceDictionary>